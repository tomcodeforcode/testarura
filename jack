    },
    {
      title: "Licensing",
      subtitle: "",
      image:
        "https://img.freepik.com/free-vector/memorandum-understanding-concept-illustration_114360-19867.jpg?t=st=1731664465~exp=1731668065~hmac=3165bf5a310bfc903c254e00568a63805e2cce40a427e786655567356baa4827&w=826",
    },
  ];

  return (
    <div>
      <section className="text-gray-700 bg-gray-50 body-font">
        <div className="py-24 flex flex-wrap items-center container mx-auto">
          <div className="md:w-1/2 md:pr-10 pl-4">
            <p className="text-lg font-medium text-black font-pj text-left">
              SERVICES
            </p>
            <h2
              style={{ color: "#4169e1" }}
              className="sm:text-5xl text-4xl title-font mb-4 text-left font-bold"
            >
              We Provide <br /> Awesome <br /> Services
            </h2>
            <p className="mb-6 leading-relaxed text-lg text-gray-600 text-left">
              Are you one of the thousands of Cloudi <br /> customers who have
              no idea?
            </p>

            <div className="flex gap-4 mt-6 justify-start">
              <button
                className="bg-gray-200 rounded-full w-10 h-10 flex justify-center items-center cursor-pointer transition-all duration-300 hover:bg-gray-300"
                onClick={scrollLeft}
              >
                â†
              </button>
              <button
                className="bg-indigo-600 text-white rounded-full w-10 h-10 flex justify-center items-center cursor-pointer transition-all duration-300 hover:bg-indigo-700"
                onClick={scrollRight}
              >
                â†’
              </button>
            </div>
          </div>

          <div
            style={{
              background:
                "linear-gradient(to bottom, #4169e1 50%,  #f9fafb  50%)",
            }}
            className="md:w-1/2 md:pl-10 mt-8 md:mt-0 w-full"
          >
            <div className="overflow-hidden w-full">
              <section className="sectionx">
                <div
                  ref={scrollContainer}
                  className="cards-wrapper flex gap-4 overflow-x-auto py-4 w-full scrollbar-hide"
                  style={{
                    overflowX: "scroll",
                    scrollbarWidth: "none",
                    msOverflowStyle: "none",
                  }}
                >
                  {cardsData.map((card, index) => (
                    <div
                      key={index}
                      className="cardx min-w-[250px] p-4 rounded-lg text-center transition-all duration-300 hover:shadow-2xl hover:scale-105 "
                    >
                      <div className="relative">
                        {/* Unique circular image for each card */}
                        <img
                          src={card.image} // Dynamic image URL from the cardsData array
                          alt={`Circle ${card.title}`}
                          className="w-32 h-32 rounded-full mx-auto" // Increased size
                        />
                      </div>

                      <h1 className="text-xl font-semibold mb-4">
                        {card.title}
                        {card.subtitle && (
                          <>
                            <br />
                            {card.subtitle}
                          </>
                        )}
                      </h1>
                    </div>
                  ))}
                </div>
              </section>
            </div>
          </div>
        </div>
      </section>
    </div>
  );
}

