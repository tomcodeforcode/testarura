import React from 'react';
import { FaTwitter, FaFacebookF, FaLinkedinIn, FaYoutube, FaPinterestP } from 'react-icons/fa';
import { SiGooglemaps } from 'react-icons/si';

function Footer() {
  const icons = [
    {
      href: '#',
      icon: <FaTwitter size={24} color="#1DA1F2" />, // Twitter's brand color
      title: 'Twitter',
    },
    {
      href: '#',
      icon: <FaFacebookF size={24} color="#316FF6" />, // Facebook's brand color
      title: 'Facebook',
    },
    {
      href: '#',
      icon: <SiGooglemaps size={24} color="blue" />, // Google Maps' brand color
      title: 'Site Map',
    },


    {
      href: '#',
      icon: <FaPinterestP size={24} color="#E60023" />, // Pinterest's brand color
      title: 'Pinterest',
    },
    {
      href: '#',
      icon: <FaLinkedinIn size={24} color="#0A66C2" />, // LinkedIn's brand color
      title: 'LinkedIn',
    },
  ];

  return (
    <section style={{ background: 'linear-gradient(to right, #6a4cfc, #878bee, #a0a7ff)' }} className="py-16 sm:pt-20 lg:pt-24">
      <div className="px-6 mx-auto max-w-7xl sm:px-8 lg:px-12">
        <div className="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-6 gap-y-16 gap-x-12">
          {/* Logo and Description */}
          <div className="col-span-2 md:col-span-3 lg:col-span-2 lg:pr-8 text-left">
            <img className="w-auto h-9" src="/cloudipreview.png" alt="Company Logo" />
            <p className="text-base text-white mt-7">
            Managing a business comes with countless responsibilities, from legal compliance and tax optimization to financial management and growth planning. Our expert team ensures legal and financial stability while offering tailored growth strategies and ongoing support. Focus on growing your business as we handle the complexities, empowering your success every step of the way.
          
            </p>

            <ul className="flex items-center space-x-4 mt-9">
              {icons.map((icon) => (
                <li key={icon.title}>
                  <a
                    href={icon.href}
                    className="flex items-center justify-center text-white transition-all duration-300 transform bg-white rounded-full w-12 h-12 hover:bg-green-600 hover:scale-110"
                    aria-label={icon.title}
                  >
                    {icon.icon}
                  </a>
                </li>
              ))}
            </ul>
          </div>

          {/* Company Links */}
          <div>
            <p className="text-sm font-bold tracking-widest text-gray-200 uppercase">Explore Our Services</p>
            <ul className="mt-6 space-y-4">
              {['About Us', 'Learning', 'Pay Now', 'Contact Us'].map((item) => (
                <li key={item} className="hover:text-blue-200 text-left">
                  <a href="#" className="text-base text-white transition-all duration-200  no-underline">
                    {item}
                  </a>
                </li>
              ))}
            </ul>
          </div>

          {/* Help Links */}
          <div>
            <p className="text-sm font-bold tracking-widest  text-gray-200  uppercase  ">Customer Support  </p>
            <ul className="mt-6 space-y-4">


              {[
                { name: 'Sitemap', link: '/view/Sitemap' },
                { name: 'Privacy Policy', link: '/view/Faq_whole' },
                { name: 'Support', link: '/support' },
                { name: 'Refund Policy', link: '/refund-policy' }
              ].map(({ name, link }) => (
                <li key={name} className="hover:text-blue-200 text-left">
                  <a href={link} className="text-base text-white transition-all duration-200 no-underline">
                    {name}
                  </a>
                </li>
              ))}
            </ul>
          </div>

          {/* Subscribe Section */}
          <div className="col-span-2 md:col-span-1 lg:col-span-2 lg:pl-8">
            <p className="text-sm font-semibold tracking-widest text-gray-200 uppercase">Subscribe to newsletter</p>
            <form action="#" method="POST" className="mt-6">
              <div className="flex" style={{ border: '1px solid white' }}>
                <div className="flex-1">
                  <label htmlFor="email" className="sr-only">Email</label>
                  <input
                    type="email"
                    name="email"
                    id="email"
                    placeholder="Enter your email"
                    className="block w-full p-4 text-black placeholder-gray-500 transition-all duration-200 bg-white border border-gray-300 rounded-l-md focus:outline-none focus:border-blue-600 caret-blue-600"
                  />
                </div>
                <button
                  type="submit"
                  // style={{ backgroundColor: 'black' }}
                  className="inline-flex items-center bg-blue-800 justify-center px-6 py-4 font-semibold text-white transition-all duration-200 rounded-r-md hover:bg-blue-800 focus:bg-blue-800"
                >
                  Subscribe
                </button>
              </div>
            </form>
          </div>
        </div>

        <hr className="mt-16 mb-10 border-gray-300 " />
        <p className="text-sm text-center text-gray-200 ">Â© Copyright 2024, All Rights Reserved by Cloudi-insta24</p>
      </div>
    </section>
  );
}

