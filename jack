import React from 'react';

function Review_strip() {
  return (
    <div className="relative font-sans before:absolute before:w-full before:h-full before:inset-0 bg-gray-50">
      {/* Background Image (optional, if you want an image you can uncomment below) */}
      {/* <img src="https://images.unsplash.com/photo-1520013817300-1f4c1cb245ef?q=80&w=2113&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" alt="Banner Image" className="absolute inset-0 w-full h-full object-cover" /> */}

      <div className="min-h-[370px] relative z-50 h-full max-w-6xl mx-auto flex flex-col justify-center items-center text-center px-6 py-12">
        <h2  style={{ color:'#4169e1'
  }}  className="text-4xl font-bold mb-12 ">Insights</h2>
        
        <div className="grid lg:grid-cols-4 sm:grid-cols-2 gap-6 max-lg:gap-12">
          {/* Stats Block 1 */}
          <div className="text-center bg-white shadow-lg rounded-lg p-6">
