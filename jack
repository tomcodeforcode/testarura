import React, { useRef } from "react";

function R1() {
  const scrollContainer = useRef(null);

  const scrollLeft = () => {
    if (scrollContainer.current) {
      scrollContainer.current.scrollBy({
        left: -300,
        behavior: "smooth",
      });
    }
  };

  const scrollRight = () => {
    if (scrollContainer.current) {
      scrollContainer.current.scrollBy({
        left: 300,
        behavior: "smooth",
      });
    }
  };

  // Array of cards with unique titles and image URLs
  const cardsData = [
    {
      title: "Startup",
      subtitle: "Registration",
      image:
        "https://img.freepik.com/free-vector/startup-life-concept-illustration_114360-1226.jpg?t=st=1731663977~exp=1731667577~hmac=8d9cc8912c400f972a5e780beb4cc081dc5501358adba575f4c21e44c28fd344&w=826",
    },
    {
      title: "Regulatory",
      subtitle: "Compliance",
      image:
        "https://img.freepik.com/free-vector/unemployment-insurance-abstract-concept-vector-illustration-unemployment-benefits-lost-job-tired-stressed-businessman-claim-form-workers-compensation-paper-work-interview-abstract-metaphor_335657-1355.jpg?t=st=1731664054~exp=1731667654~hmac=ad5cb60dc02dfc26be40c31e1fe08e0dc01800bcff3203f5dd417fb1b37e8a20&w=826",
    },
    {
      title: "Advisory and",
      subtitle: "Consultancy",
      image:
        "https://img.freepik.com/free-vector/consulting-concept-illustration_114360-2719.jpg?t=st=1731664127~exp=1731667727~hmac=625587aa54270ce0b814ed5124124e1775d82a8b2db7fbce49066b6002724f71&w=826",
    },
    {
      title: "Tax and",
      subtitle: "Auditing",
      image:
        "https://img.freepik.com/free-vector/tax-concept-illustration_114360-5984.jpg?t=st=1731664170~exp=1731667770~hmac=2def6d1975db3c1506dbf72d523b3444bff13fc5c65e024273c255430873b3db&w=826",
    },
    {
      title: "Business",
      subtitle: "Templates",
      image:
        "https://img.freepik.com/free-vector/flat-graphic-designer-logo-collection_23-2148844187.jpg?t=st=1731664337~exp=1731667937~hmac=6daa8a7ec5d476928aa70c870adbeeb9697bc7de3c971ae2989cc69429d76f47&w=826",
    },
    {
      title: "CA/CS",
      subtitle: "Services",
      image:
