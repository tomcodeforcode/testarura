import React, { useState, useEffect } from "react";
import { useParams } from "react-router-dom";
import { contentData } from "../components/Faq_whole_datasheet";
import { IoPersonCircleSharp } from "react-icons/io5";
import { MdOutlineEmail } from "react-icons/md";

import Footer from "../components/footer";
import CompanyLogos from "../components/company_logos";
import Review_strip from "../components/Review_strip";
import Review_section from "../components/Review_section";

const ContentDisplay = () => {
  const { title } = useParams();
  const content = contentData[title];

  const [openAccordion, setOpenAccordion] = useState(null);

  const toggleAccordion = (index) => {
    // If the clicked accordion is already open, close it; otherwise, open it
    setOpenAccordion(openAccordion === index ? null : index);
  };

  useEffect(() => {
    window.scrollTo(0, 0);
  }, []);

  // If the content is not found, display a "Not Found" message
  if (!content) {
    return (
      <div className="container mx-auto p-6 ">
        <div className="faq-content p-6 bg-white rounded-lg shadow-xl mt-8">
          <h2 className="text-2xl font-bold text-center text-red-500">
            FAQ Not Found
          </h2>
          <p className="mt-4 text-gray-600 text-center">
            Sorry, the content you are looking for is not available.
          </p>
        </div>
      </div>
    );
  }

  return (
    <>
      <div
        style={{ marginTop: "70px" }}
        className="font-sans bg-white py-12 px-4 bg-gradient-to-r from-gray-200 via-gray-50 to-white"
      >
        <div className="max-w-7xl mx-auto text-center sm:text-left">
          {/* Content Section */}
          <div className="grid md:grid-cols-2 gap-12 mt-6">
            {/* Left Side: Heading, Content, Button */}
            <div>
              {/* Heading Section */}
              {content.map((item, index) => {
                if (item.type === "heading") {
                  return (
                    <h2 className="text-blue-800 lg:text-5xl md:text-4xl text-2xl font-extrabold lg:leading-[64px] md:max-w-4xl text-left">
                      {item.heading}
                    </h2>
                  );
                }
                return null;
              })}

              {/* Content Section */}
              {content.map((item, index) => {
                if (item.type === "content") {
                  return (
                    <p className="text-gray-600 text-base leading-relaxed text-left">
                      {item.content}
                    </p>
                  );
                }
                return null;
              })}

              <div className="mt-12 flex gap-4 items-center justify-center sm:justify-start">
                <button
                  type="button"
                  className="bg-blue-600 hover:bg-blue-700 transition-all text-white font-bold text-sm rounded-full px-6 py-3"
                >
                  Schedule Free Consultation
                </button>
              </div>
            </div>

            {/* Right Side: Form Section */}
            <div className="ml-auto max-md:mx-auto md:mt-0">
              <div className="bg-gray-50 border border-gray-300 rounded-lg p-6 max-w-md shadow-lg">
                <form className="space-y-4">
                  {/* Price Section */}
                  <div className="relative flex items-center bg-gradient-to-r from-white via-blue-500 to-blue-800 shadow-xl rounded-full font-sans p-3">
                    <span className="bg-gradient-to-br from-yellow-400 to-yellow-600 text-white text-xl font-semibold rounded-full w-20 h-20 flex items-center justify-center shadow-lg absolute left-0 transform -translate-x-1/2 hover:scale-110 transition-all duration-300">
                      50%
                    </span>
                    <h3 className="text-white text-xl font-bold mx-6 uppercase tracking-wide relative left-10">
                      Affordable Price
                    </h3>
                    <p className="text-gray-300 text-sm font-medium mt-2 relative left-10 opacity-90 hover:opacity-100 transition-all duration-300">
                      Money Back Guarantee
                    </p>
                  </div>

                  <div className="mb-8">
                    <h5 className="text-gray-800 font-extrabold">
                      Expert Insights, Free Consultation â€“ Book Now!
                    </h5>
                  </div>

                  {/* Form Fields */}
                  <div className="space-y-4">
                    {/* Name Input */}
                    <div className="relative flex items-center">
                      <input
                        name="username"
                        type="text"
                        required
                        className="w-full text-sm text-gray-800 border border-gray-300 px-4 py-3 rounded-lg outline-blue-600"
                        placeholder="Your Name"
                      />
                      <svg
                        fill="#bbb"
                        stroke="#bbb"
                        className="w-[18px] h-[18px] absolute right-4"
                        viewBox="0 0 24 24"
                      >
                        <IoPersonCircleSharp style={{ fontSize: "25px" }} />
                      </svg>
                    </div>

                    {/* Email Input */}
                    <div className="relative flex items-center">
                      <input
                        name="password"
                        type="password"
                        required
                        className="w-full text-sm text-gray-800 border border-gray-300 px-4 py-3 rounded-lg outline-blue-600"
                        placeholder="Your Email"
                      />
                      <svg
                        fill="#bbb"
                        stroke="#bbb"
                        className="w-[18px] h-[18px] absolute right-4 cursor-pointer"
                        viewBox=""
                      >
                        <MdOutlineEmail style={{ fontSize: "25px" }} />
                      </svg>
                    </div>

                    {/* Phone Number Inputs */}
