          {/* Loop through each item in content */}
          {content.map((item, index) => {
            // Dynamically render FAQ or heading based on item type
            if (item.type === "faq") {
              return (
                <div
                  key={index}
                  className="shadow-[0_2px_10px_-3px_rgba(6,81,237,0.3)] rounded-lg border-l-8 border-blue-600  mt-16"
